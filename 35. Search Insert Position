class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int size = nums.size();
        if(size == 0) return 0;
        
        int res = 0;
        for(int i = 0; i < size; ++i){
            if(nums[i] >= target) return res;
            else  res++;       
        }
        return res;
    }
};
